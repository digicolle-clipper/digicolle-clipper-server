extends ./layout.jade

block append css
  meta(name='apple-mobile-web-app-capable', content='yes')
  meta(name='viewport', content='width=device-width,initial-scale=1,minimun-scale=1,user-scalable=no')
  link(rel='stylesheet', href='/css/tinder.css')
  script(src='/js/jquery.min.js')

block nav

block contents
  div.row
    ul
      each photo in photos
        li
          div#img-wrapper
            img(src=photo.photo_url)

block footer
  script(src='/js/swing.js')
  script(src='/js/tinder.js')
